<?php
function hello(){
    return 'Hello';
}
function world(){
    return "World!\n";
}
/* some variables */
$fn1 = 'hello';
$fn2 = 'world';
echo $fn1() . ' ' . $fn2();
// example class
class foo extends bar{
	function __construct() {
		$doo = "wah dee $something dee ";
	}
	public static function mystaticfunc() {
		$dee = "dee dee dum";
	}
}
?>
